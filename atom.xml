<pre><code class="language-pascal">var
  a,b,c,d:array[0..300] of integer;//定义abcd数组
  i,j,k,n:longint;//定义一些杂七杂八的东西
begin
  readln(n);//读入n
  if n=1 then begin writeln(2);exit;end;
  if n=2 then begin writeln(4);exit;end;
  if n=3 then begin writeln(7);exit;end;
  //以上依旧是特判
  a[1]:=2;b[1]:=4;c[1]:=7;//初始化
  for i:=4 to n do//循环
    begin
      k:=0;//进位清零
      for j:=1 to 300 do//每个位数处理一下
        begin
          d[j]:=a[j]+b[j]+c[j]+k;//加起来
          k:=d[j] div 10;d[j]:=d[j] mod 10;处理进位
        end;
      a:=b;b:=c;c:=d;//交换
    end;
  k:=300;while (d[k]=0) and (k&gt;1) do dec(k);//查找非零的位数
  for i:=k downto 1 do write(d[i]);//输出
end.
